{
  "name": "backend-starter-ts",
  "version": "1.0.0",
  "description": "backend-starter-ts",
  "main": "./dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "npm run build && cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "rm:all": "rm -rf node_modules dist yarn.lock package-lock.json",
    "reset": "rm -rf node_modules dist yarn.lock package-lock.json && yarn install &&  tsc ",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:models": "jest --selectProjects models",
    "test:integration": "jest --selectProjects integration",
    "test:utils": "jest --selectProjects utils",
    "test:models:watch": "jest --selectProjects models --watch",
    "test:integration:watch": "jest --selectProjects integration --watch",
    "test:generate": "node scripts/generate-tests.js generate",
    "test:generate-model": "node scripts/generate-tests.js generate-model",
    "test:help": "node scripts/generate-tests.js help",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "commit": "git add . && cz",
    "push": "git push",
    "commit-and-push": "yarn commit && tsc && git push"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/s3-request-presigner": "^3.782.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^22.14.0",
    "amqplib": "^0.10.7",
    "bullmq": "^5.48.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "i18next": "^24.2.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.2",
    "ioredis": "^5.6.0",
    "joi": "^17.13.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/amqplib": "^0.10.7",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "commitizen": "^4.3.1",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^9.25.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.3.1",
    "ts-node-dev": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
